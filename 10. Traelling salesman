#include <stdio.h>
int tsp_g [10][10] = {{12,30,33,10,45}
{56,22,9,15,18}
29,13,8,5,12}
{33,28,16,10,3}
{1,4,30,24,20}
};
int visited[10],n,cost=0;
void travelling salesman(int c){
    int k, adj_vertex = 999;
    int min = 999;

    visited[c]=1
    print("%d", c+1);
    for(k=0;k<n;k++){
        if(tsp_g[c][k]!=0)
        &&(visited[k] == 0}{
            if(tsp_g [c][k] < min){
                min = tsp_g[c][k];
                adj_vertex = k;

            }
        }
    }
}
output :
shortest path : 1 4 5 2 3 
Minimum cost : 55
